{
    "name": "time2l-lambda-approvals",
    "version": "0.0.1",
    "description": "Serverless approvals for Time2L",
    "scripts": {
        "dep-graph": "npx madge --image dependency-graph.svg src/",
        "deploy": "NODE_ENV=dev yarn deploy:env",
        "deploy:env": "npx sls deploy --stage $NODE_ENV",
        "deploy:production": "NODE_ENV=production yarn deploy:env",
        "lint": "eslint ./src",
        "preinstall": "node -e 'if(!/yarn\\.js$/.test(process.env.npm_execpath))throw new Error(\"Please use `yarn install` instead of `npm install`\")'",
        "serve": "npx sls offline start --stage local --local --watch",
        "next": "next dev -p 3000",
        "next-build": "next build"
    },
    "author": "",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://bitbucket.org/hikey-resources/time2l-lambda-approvals"
    },
    "engines": {
        "node": ">=10.13 <=10.17"
    },
    "devDependencies": {
        "@babel/core": "^7.7.2",
        "@babel/plugin-proposal-object-rest-spread": "7.6.2",
        "@babel/plugin-transform-runtime": "^7.6.2",
        "@babel/preset-env": "^7.7.1",
        "@babel/register": "^7.7.0",
        "babel-loader": "^8.0.6",
        "babel-plugin-source-map-support": "^2.1.1",
        "chai": "^4.2.0",
        "dotenv": "^8.2.0",
        "eslint": "^6.6.0",
        "eslint-config-airbnb": "^18.0.1",
        "eslint-config-prettier": "^6.5.0",
        "eslint-plugin-import": "^2.18.2",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-mocha": "^6.2.1",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-react": "^7.16.0",
        "file-loader": "^4.2.0",
        "husky": "^3.0.9",
        "lint-staged": "^9.4.2",
        "madge": "^3.6.0",
        "mocha": "^6.2.2",
        "nyc": "^14.1.1",
        "prettier": "^1.19.1",
        "serverless-content-encoding": "^1.1.0",
        "serverless-domain-manager": "^3.3.0",
        "serverless-nextjs-plugin": "^2.4.2",
        "serverless-offline": "^5.12.0",
        "serverless-plugin-split-stacks": "^1.9.2",
        "serverless-plugin-tracing": "^2.0.0",
        "serverless-plugin-warmup": "^4.7.1-rc.1",
        "serverless-prune-plugin": "^1.4.1",
        "serverless-webpack": "^5.3.1",
        "sinon": "^7.5.0",
        "webpack": "^4.41.2",
        "webpack-node-externals": "^1.7.2"
    },
    "dependencies": {
        "@babel/runtime": "^7.7.2",
        "@hapi/joi": "^16.1.7",
        "@hapi/joi-date": "^2.0.1",
        "@material-ui/core": "4.6.1",
        "@material-ui/icons": "4.5.1",
        "aws-sdk": "^2.570.0",
        "clsx": "1.0.4",
        "moment": "^2.24.0",
        "next": "^8.1.0",
        "next-aws-lambda": "latest",
        "prop-types": "15.7.2",
        "react": "^16.11.0",
        "react-dom": "^16.11.0",
        "serverless": "^1.57.0",
        "source-map-support": "^0.5.16"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "test/**/*.js": [
            "npx prettier --write",
            "npx yarn lint --fix",
            "git add"
        ],
        "spec/**/*.js": [
            "npx prettier --write",
            "npx yarn lint --fix",
            "git add"
        ],
        "src/**/*.js": [
            "npx prettier --write",
            "npx yarn lint --fix",
            "git add"
        ],
        "**/*.md": [
            "npx prettier --write",
            "git add"
        ]
    }
}
